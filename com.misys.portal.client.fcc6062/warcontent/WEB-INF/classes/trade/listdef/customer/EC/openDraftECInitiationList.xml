<?xml version="1.0" encoding="UTF-8"?>
<listdef ordered="y" default_order="ref_id" default_order_type="d" page="50">

	<column name="ref_id" align="center" width="11em" loca_key="REFERENCEID">
		<url>
			<parameter name="SCREEN" value="EC_SCREEN"/>
			<parameter name="TRANSACTION_TYPE_CODE" business_code="N002_NEW"/>
			<parameter name="MODE_CODE" binding_code="MODE_DRAFT"/>
			<parameter name="REFERENCE_ID" column="ref_id"/>
			<parameter name="TRANSACTION_ID" column="tnx_id"/>
		</url>
	</column>
	<column name="entity" align="left" width="20%" loca_key="ENTITY"/>
	<column name="drawer_name" align="left" width="30%" loca_key="ENTITY_NAME_DRAWER"/>
	<column name="drawee_name" align="left" width="40%" loca_key="DRAWEE_NAME"/>
	<column name="cur_code" align="center" width="4em" loca_key="CURCODE"/>
	<column name="tnx_amt" align="right" width="10em" loca_key="AMOUNT"/>
	<column name="full name" computation="sum" align="left" width="40%" loca_key="USER_NAME">
		<column name="Inputter@first_name"/>	
				<column name="Inputter@last_name"/>
	</column>
	<column name="users" width="3em" loca_key="USERS">
		<url usersAction="y">
		<!-- Mandatory:- REFERENCE_ID,TRANSACTION_ID,PRODUCT_CODE
			 Optional :- TRANSACTION_TYPE_CODE -->
			<parameter name="REFERENCE_ID" column="ref_id"/>
			<parameter name="TRANSACTION_ID" column="tnx_id"/>
			<parameter name="PRODUCT_CODE" column="product_code"/>
			<parameter name="TRANSACTION_TYPE_CODE" column="tnx_type_code"/>
		</url>
	</column>
	
	<column name="Inputter@last_name" hidden="y"/>
	<column name="Inputter@first_name" hidden="y"/>
	<column name="inp_dttm" align="center" width="6em" loca_key="INP_DTTM"/>

	<column name="tnx_id" hidden="y"/>
	<column name="product_code" hidden="y"/>

	<!-- Parameters -->
	<parameter
		name="ref_id"
		size="14"
		max_length="20"
		loca_key="REFERENCEID"
		input="y"
		default="">
	</parameter>
	<parameter
		name="entity"
		size="20"
		max_length="35"
		loca_key="ENTITY"
		input="y"
		default=""
		type="entity"/>
	<parameter
		name="drawer_name"
		size="35"
		max_length="35"
		loca_key="ENTITY_NAME_DRAWER"
		input="y"
		default=""
		config_maxLength=""
		/>
	<parameter
		name="drawee_name"
		loca_key="DRAWEE_NAME"
		size="20"
		max_length="35"
		input="y"
		config_maxLength=""/>
	<parameter
		name="user_name"
		loca_key="USER_NAME"
		size="20"
		max_length="70"
		input="y"/>
	<parameter
		name="cur_code"
		input="y"
		size="3"
		max_length="3"
		loca_key="CURCODE"
		type="currency"/>

	<!-- Candidates -->	
	
	<!-- pending files -->
	<candidate name="export_collection">
		<filter>
			<criteria>
				<column name="tnx_stat_code"/>
				<operator type="equal"/>
				<value type="code">
					N004_INCOMPLETE
				</value>
			</criteria>
		</filter>
		<filter>
			<criteria>
				<column name="tnx_stat_code"/>
				<operator type="equal"/>
				<value type="code">
					N004_UNCONTROLLED
				</value>
			</criteria>
		</filter>
	</candidate>
	
	<!-- in progress files -->
	<candidate name="export_collection">
		<filter>
			<criteria>
				<column name="tnx_stat_code"/>
				<operator type="equal"/>
				<value type="code">
					N004_ACKNOWLEDGED
				</value>
			</criteria>
			<criteria>
				<column name="prod_stat_code"/>
				<operator type="equal"/>
				<value type="code">
					N005_INPROGRESS
				</value>
			</criteria>
		</filter>
	</candidate>
	
	<!-- Common filters -->
	<filter>
		<criteria>
			<column name="ref_id"/>
			<operator type="like"/>
			<value type="parameter">
				ref_id
			</value>
		</criteria>
		<criteria>
			<column name="entity"/>
			<operator type="like"/>
			<value type="parameter">
				entity
			</value>
		</criteria>
		<criteria>
			<column name="drawer_name"/>
				<operator type="like"/>
				<value type="parameter">
					drawer_name
				</value>
			</criteria>
		<criteria>
			<column name="drawee_name"/>
			<operator type="like"/>
			<value type="parameter">
				drawee_name
			</value>
		</criteria>
		<criteria>
			<column name="cur_code"/>
			<operator type="like"/>
			<value type="parameter">
				cur_code
			</value>
		</criteria>
	</filter>
	
	<filter or="y">
		<criteria>
			<column name="full_name" computation="sum">
				<column name="Inputter@first_name"/>	
				<column name="Inputter@last_name"/>
			</column>
			<operator type="like"/>
			<value type="parameter">
				user_name
			</value>
		</criteria>
	</filter>
	
	<filter>
		<businesscriteria type="isNew"/>
	</filter>
</listdef>
