<?xml version="1.0" encoding="UTF-8"?>
<listdef ordered="y" huge="y" help="LIST_HELP_INQUIRY" default_order="ref_id" default_order_type="d" page="50">

	<column name="action" width="3em">
		<url action="y">
			<parameter name="SCREEN" value="LS_SCREEN"/>
			<parameter name="REFERENCE_ID" column="ref_id"/>
			<parameter name="PRODUCT_CODE" column="product_code"/>
			<parameter name="OPTION_CODE" binding_code="OPTION_FULL"/>
		</url>
	</column>
	<column name="ref_id" align="center" width="10em" loca_key="REFERENCEID"/>
	<column name="entity" align="left" width="25%" loca_key="ENTITY"/>
	<column name="bo_ref_id" align="left" width="15%" loca_key="BO_REFERENCEID"/>
	<column name="cust_ref_id" align="left" width="23%" loca_key="CUST_REFERENCEID"/>
	<column name="beneficiary_name" align="left" width="20%" loca_key="BENEFICIARY_NAME"/>
	<column name="ls_type" align="left" width="10%" loca_key="LICENSE_TYPE"/>
	<column name="cur_code" align="center" width="3em" loca_key="CURCODE"/>
	<column name="amt" align="right" width="10em" loca_key="AMOUNT"/>
	<column name="iss_date" align="center" width="6em" loca_key="ISS_DATE"/>
	<column name="valid_to_date" align="center" width="6em" loca_key="VALID_TO_DATE"/>
	<column name="prod_stat_code" align="center" width="7%" loca_key="STATUS" sort="n"/>

	<column name="product_code" hidden="y"/>

	<!-- parameters -->
	<parameter
		name="ref_id"
		size="14"
		max_length="20"
		loca_key="REFERENCEID"
		input="y"
		default=""/>
	<parameter
		name="entity"
		size="35"
		max_length="35"
		loca_key="ENTITY"
		input="y"
		default=""
		type="entity"/>
	<parameter
		name="bo_ref_id"
		size="20"
		max_length="35"
		loca_key="BO_REFERENCEID"
		input="y"
		default=""/>
	<parameter
		name="cust_ref_id"
		size="20"
		max_length="35"
		loca_key="CUST_REFERENCEID"
		input="y"
		default=""/>
	<parameter
		name="beneficiary_name"
		loca_key="BENEFICIARY_NAME"
		size="35"
		max_length="35"
		input="y"/>
	<parameter
		name="ls_type"
		type="LicenseTypes"
		size=""
		max_length=""
		loca_key="LICENSE_TYPE"
		input="y"
		default=""/>
	<parameter
		name="cur_code"
		input="y"
		size="3"
		max_length="3"
		loca_key="CURCODE"
		type="currency"/>
	<parameter
		name="export_list"
		file_name="inquiryLS"
		type="export">
	</parameter>

	<!-- candidates -->	
	<candidate name="license_master">
		<filter>
			<criteria>
				<column name="ref_id"/>
				<operator type="like"/>
				<value type="parameter">
					ref_id
				</value>
			</criteria>
			<criteria>
				<column name="ls_type"/>
				<operator type="like"/>
				<value type="parameter">
					ls_type
				</value>
			</criteria>
			<criteria>
				<column name="bo_ref_id"/>
				<operator type="like"/>
				<value type="parameter">
					bo_ref_id
				</value>
			</criteria>
			<criteria>
				<column name="cust_ref_id"/>
				<operator type="like"/>
				<value type="parameter">
					cust_ref_id
				</value>
			</criteria>
			<criteria>
				<column name="entity"/>
				<operator type="like"/>
				<value type="parameter">
					entity
				</value>
			</criteria>
			<criteria>
				<column name="beneficiary_name"/>
				<operator type="like"/>
				<value type="parameter">
					beneficiary_name
				</value>
			</criteria>
			<criteria>
				<column name="cur_code"/>
				<operator type="like"/>
				<value type="parameter">
					cur_code
				</value>
			</criteria>
			<criteria>
				<column name="prod_stat_code"/>
				<operator type="different"/>
				<value type="code">
					N005_PENDING
				</value>
			</criteria>
			<criteria>
				<column name="prod_stat_code"/>
				<operator type="different"/>
				<value type="code">
					N005_PURGED
				</value>
			</criteria>
		</filter>	
	</candidate>

</listdef>
