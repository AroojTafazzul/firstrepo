<?xml version="1.0" encoding="UTF-8"?>
<listdef ordered="y" default_order="ref_id" default_order_type="d" page="50" js_binding="misys.binding.common.listdef_mc">

	<column name="ref_id" align="center" width="10em" loca_key="REFERENCEID">
		<url>
			<parameter name="SCREEN" value="LS_SCREEN"/>
			<parameter name="TRANSACTION_TYPE_CODE" business_code="N002_INQUIRE"/>
			<parameter name="OPTION_CODE" binding_code="OPTION_ACTION_REQUIRED"/>
			<parameter name="REFERENCE_ID" column="ref_id"/>
			<parameter name="TRANSACTION_ID" column="tnx_id"/>
		</url>
	</column>
	<column name="entity" align="left" width="25%" loca_key="ENTITY"/>
	<column name="ls_type" align="left" width="15%" loca_key="LICENSE_TYPE"/>
	<column name="ls_number" align="left" width="15%" loca_key="LICENSE_NUMBER"/>
	<column name="bo_ref_id" align="left" width="20%" loca_key="BO_REFERENCEID"/>
	<column name="cur_code" align="center" width="4em" loca_key="CURCODE"/>
	<column name="amt" align="right" width="10em" loca_key="AMOUNT"/>
	<column name="action_req_code" align="left" width="25%" loca_key="TYPE"/>
	<column name="valid_to_date" align="center" width="6em" loca_key="VALID_TO_DATE"/>
	<column name="prod_stat_code" align="left" width="15%" loca_key="LABEL_STATUS" sort="n"/>
	
	<column name="tnx_id" hidden="y"/>
	<column name="product_code" hidden="y"/>
	
	<!-- parameters -->
	<parameter
		name="ref_id"
		size="14"
		max_length="20"
		loca_key="REFERENCEID"
		input="y"
		default=""/>
	<parameter
		name="entity"
		size="35"
		max_length="35"
		loca_key="ENTITY"
		input="y"
		default=""
		type="Entity"/>
	<parameter
		name="ls_number"
		size="14"
		max_length="20"
		loca_key="LICENSE_NUMBER"
		input="y"
		default=""/>
	<parameter
		name="ls_type"
		type="LicenseTypes"
		size=""
		max_length=""
		loca_key="LICENSE_TYPE"
		input="y"
		default=""/>
	<parameter
		name="bo_ref_id"
		loca_key="BO_REFERENCEID"
		size="20"
		max_length="35"
		input="y"/>
	<parameter
		name="cur_code"
		input="y"
		size="3"
		max_length="3"
		loca_key="CURCODE"
		type="Currency"/>

	<!-- candidates -->	
	<candidate name="license">
		<filter>
			<criteria>
				<column name="ref_id"/>
				<operator type="like"/>
				<value type="parameter">
					ref_id
				</value>
			</criteria>
			<criteria>
				<column name="entity"/>
				<operator type="like"/>
				<value type="parameter">
					entity
				</value>
			</criteria>
			<criteria>
				<column name="bo_ref_id"/>
				<operator type="like"/>
				<value type="parameter">
					bo_ref_id
				</value>
			</criteria>
			<criteria>
				<column name="cur_code"/>
				<operator type="like"/>
				<value type="parameter">
					cur_code
				</value>
			</criteria>
			<criteria>
				<column name="ls_number"/>
				<operator type="like"/>
				<value type="parameter">
					ls_number
				</value>
			</criteria>
			<criteria>
				<column name="ls_type"/>
				<operator type="like"/>
				<value type="parameter">
					ls_type
				</value>
			</criteria>
			<!-- criteria>
				<column name="action_req_code"/>
				<operator type="equal"/>
				<value type="parameter">
					action_req_code
				</value>
			</criteria-->
		</filter>
	</candidate>
	
	<filter>
		<criteria>
			<column name="CrossReference@child_tnx_id"/>
			<operator type="isNull"/>
		</criteria>
		<criteria>
			<column name="CrossReference@type_code"/>
			<operator type="equal"/>
			<value type="code">
				N043_ACTION_REQUIRED
			</value>
		</criteria>
	</filter>
	
</listdef>
