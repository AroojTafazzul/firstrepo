					</xs:element>
					<xs:element minOccurs="0" ref="sec_beneficiary_address_line_2">
						<xs:annotation>
							<xs:documentation>Second Beneficiary second address 
								line (Transfer).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="sec_beneficiary_dom">
						<xs:annotation>
							<xs:documentation>Second Beneficiary 
								domicile (Transfer).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="sec_beneficiary_address_line_4">
						<xs:annotation>
							<xs:documentation>Second Beneficiary fourth address 
								line (Transfer).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="sec_beneficiary_country">
						<xs:annotation>
							<xs:documentation>Second Beneficiary 
								Country (Transfer).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="sec_beneficiary_reference">
						<xs:annotation>
							<xs:documentation>Second Beneficiary 
								reference (Transfer).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="assignee_name">
						<xs:annotation>
							<xs:documentation>Second Beneficiary 
								name (Assignee).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="assignee_address_line_1">
						<xs:annotation>
							<xs:documentation>Second Beneficiary first address 
								line (Assignee).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="assignee_address_line_2">
						<xs:annotation>
							<xs:documentation>Second Beneficiary second address 
								line (Assignee).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="assignee_dom">
						<xs:annotation>
							<xs:documentation>Second Beneficiary 
								domicile (Assignee).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="assignee_address_line_4">
						<xs:annotation>
							<xs:documentation>Second Beneficiary fourth address 
								line (Assignee).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="assignee_country">
						<xs:annotation>
							<xs:documentation>Second Beneficiary 
								country (Assignee).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="assignee_reference">
						<xs:annotation>
							<xs:documentation>Second Beneficiary 
								reference (Assignee).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="applicant_abbv_name">
						<xs:annotation>
							<xs:documentation>Applicant abbreviated 
								name.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="applicant_name">
						<xs:annotation>
							<xs:documentation>Applicant name.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" ref="applicant_address_line_1">
