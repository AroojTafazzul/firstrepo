<!ELEMENT DocumentsSentToBuyer (documentsIncluded,AdditionalDocumentInformation?)>
<!ELEMENT AdditionalDocumentInformation (line+)>
<!ELEMENT AdditionalInformation (line+)>
<!ELEMENT cmp:RID (#PCDATA) >
