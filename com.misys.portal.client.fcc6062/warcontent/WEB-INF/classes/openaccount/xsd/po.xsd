<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:sch="http://www.ascc.net/xml/schematron" 
	elementFormDefault="qualified">
	<xs:annotation>
		<xs:documentation xml:lang="en"> Schema W3C for PO. XML validation 
			Copyright (c) 2000-2006 NEOMAlogic (http://www.neomalogic.com), All 
			Rights Reserved. </xs:documentation>
	</xs:annotation>
	<xs:include schemaLocation="http://www.misys.com/portal/interfaces/xsd/baseline_common.xsd"/> 
	<xs:element name="po_tnx_record">
		<xs:annotation>
			<xs:documentation>Purchase Order Business Rules</xs:documentation>
			<xs:appinfo>

				<!-- PO - Check transaction type code -->
				<sch:pattern 
					name="PO - Check the transaction type code" 
					xmlns:sch="http://www.ascc.net/xml/schematron">
					<sch:rule context="/po_tnx_record">
						<sch:assert 
							test="tnx_type_code = '01' or tnx_type_code = '03' or tnx_type_code = '13'">
							The transaction type code <value-of select="tnx_type_code"/> is not authorised.
						</sch:assert>
					</sch:rule>
				</sch:pattern> 

				<!-- ********************* -->
				<!-- PO New Business Rules -->
				<!-- ********************* -->

				<!-- PO New mandatory fields -->
				<sch:pattern 
					name="PO New - Mandatory fields " 
					xmlns:sch="http://www.ascc.net/xml/schematron">
					<sch:rule context="/po_tnx_record">
						<sch:report 
							test="tnx_type_code = '01' and
									((not(company_name) or company_name = '')
									or (not(tnx_stat_code) or tnx_stat_code = '')
									or (not(product_code) or product_code = '')
									or (not(appl_date) or appl_date = '')
									or (not(exp_date) or exp_date = '')
									or (not(total_net_cur_code) or total_net_cur_code = '')
									or (not(total_net_amt) or total_net_amt = '')
									or (not(total_cur_code) or total_cur_code = '')
									or (not(total_amt) or total_amt = '')
									or (not(buyer_name) or buyer_name = '')
									or (not(buyer_post_code) or buyer_post_code = '')
									or (not(buyer_town) or buyer_town = '')
									or (not(buyer_country) or buyer_country = '')
									or (not(seller_name) or seller_name = '')
									or (not(seller_post_code) or seller_post_code = '')
									or (not(seller_town) or seller_town = '')
									or (not(seller_country) or seller_country = '')
									or (not(issuing_bank/abbv_name) or issuing_bank/abbv_name = '')
									or (not(buyer_bank/abbv_name) or buyer_bank/abbv_name = '')
									or (not(seller_bank/abbv_name) or seller_bank/abbv_name = '')
									or (not(part_ship) or part_ship='')
									or not(line_items/lt_tnx_record)
									or not(payments/payment)
									or not(line_items/lt_tnx_record/po_ref_id))">
							Some mandatory elements are missing. 
						</sch:report>
					</sch:rule>
				</sch:pattern>
				
				<!-- PO New not authorised fields -->
				<sch:pattern 
					name="PO New - Not authorised fields " 
					xmlns:sch="http://www.ascc.net/xml/schematron">
					<sch:rule context="/po_tnx_record">
						<sch:report 
							test="tnx_type_code = '01' and
									(ref_id 
									or bo_ref_id 
									or tnx_id 
									or group_id
									or company_id 
									or iss_date
									or inp_user_id 
									or inp_dttm 
									or ctl_user_id 
									or ctl_dttm 
									or release_user_id 
									or release_dttm 
									or bo_inp_user_id 
									or bo_inp_dttm 
									or bo_ctl_user_id 
									or bo_ctl_dttm 
									or bo_release_user_id 
									or bo_release_dttm 
									or bo_comment
									or charges)">
							A not authorised element is defined. 
						</sch:report>
					</sch:rule>
				</sch:pattern>

				<!-- PO New - Constraints -->
				<sch:pattern 
					name="PO New - Check the issue date is posterior or equal to the application date" 
					xmlns:sch="http://www.ascc.net/xml/schematron">
					<sch:rule context="/po_tnx_record">
						<sch:report 
							test="tnx_type_code = '01' and
								(number(concat(concat(substring(appl_date,7,4), substring(appl_date,4,2)), substring(appl_date,1,2))) &gt;
								number(concat(concat(substring(iss_date,7,4), substring(iss_date,4,2)), substring(iss_date,1,2))))">
							The issue date must be posterior or equal to the application date.
						</sch:report>
					</sch:rule>
				</sch:pattern>
				<sch:pattern 
					name="PO New - Check the last shipment date is posterior or equal to the application date" 
					xmlns:sch="http://www.ascc.net/xml/schematron">
					<sch:rule context="/po_tnx_record">
						<sch:report 
							test="tnx_type_code = '01' and last_ship_date != '' and
								(number(concat(concat(substring(last_ship_date,7,4), substring(last_ship_date,4,2)), substring(last_ship_date,1,2))) &lt;
								number(concat(concat(substring(appl_date,7,4), substring(appl_date,4,2)), substring(appl_date,1,2))))">
							The last shipment date must be posterior or equal to the application date.
						</sch:report>
					</sch:rule>
				</sch:pattern>
				
				<!-- *********************** -->
				<!-- PO Amend Business Rules -->
				<!-- *********************** -->

				<!-- PO Amend mandatory fields -->
				<sch:pattern 
					name="PO Amend - Mandatory fields " 
					xmlns:sch="http://www.ascc.net/xml/schematron">
					<sch:rule context="/po_tnx_record">
						<sch:report 
							test="tnx_type_code = '03' and
									((not(company_name) or company_name = '')
									or (not(tnx_stat_code) or tnx_stat_code = '')
									or (not(product_code) or product_code = '')
									or (not(sub_tnx_type_code) or sub_tnx_type_code = ''))">
							Some mandatory elements are missing. 
						</sch:report>
					</sch:rule>
				</sch:pattern>
				
				<!-- PO Amend not authorised fields -->
				<sch:pattern 
					name="PO Amend - Not authorised fields " 
					xmlns:sch="http://www.ascc.net/xml/schematron">
					<sch:rule context="/po_tnx_record">
						<sch:report 
							test="tnx_type_code = '03' and
									(template_id
									or tnx_id
									or company_id
									or entity
									or inp_user_id 
									or inp_dttm 
									or ctl_user_id 
									or ctl_dttm 
									or release_user_id 
									or release_dttm 
									or bo_inp_user_id 
									or bo_inp_dttm 
									or bo_ctl_user_id 
									or bo_ctl_dttm 
									or bo_release_user_id 
									or bo_release_dttm
									or appl_date
									or iss_date
									or last_ship_date
									or tnx_cur_code
									or tnx_amt
									or issuer_ref_id
									or buyer_abbv_name
									or buyer_name
									or buyer_bei
									or buyer_bank_bic
									or buyer_street_name
									or buyer_post_code
									or buyer_town_name
									or buyer_country_sub_div
									or buyer_country
									or buyer_reference
									or seller_abbv_name
									or seller_name
									or seller_bei
									or seller_bank_bic
									or seller_street_name
									or seller_post_code
									or seller_town_name
									or seller_country_sub_div
									or seller_reference
									or seller_country
									or bill_to_abbv_name
									or bill_to_name
									or bill_to_bei
									or bill_to_street_name
									or bill_to_post_code
									or bill_to_town_name
									or bill_to_country_sub_div
									or bill_to_country
									or ship_to_abbv_name
									or ship_to_name
									or ship_to_bei
									or ship_to_street_name
									or ship_to_post_code
									or ship_to_town_name
									or ship_to_country_sub_div
									or ship_to_country 
									or consgn_abbv_name
									or consgn_name
									or consgn_bei
									or consgn_street_name
									or consgn_post_code
									or consgn_town_name
									or consgn_country_sub_div
									or consgn_country
									or goods_desc
									or part_ship
									or tran_ship
									or nb_mismatch
									or full_match
									or total_amt
									or total_cur_code
									or total_net_amt
									or total_net_cur_code
									or order_total_amt
									or order_total_cur_code
									or order_total_net_amt
									or order_total_net_cur_code
									or accpt_total_amt
									or accpt_total_cur_code
									or accpt_total_net_amt
									or accpt_total_net_cur_code
									or liab_total_amt
									or liab_total_cur_code
									or liab_total_net_amt
									or liab_total_net_cur_code
									or fin_inst_bic
									or fin_inst_name
									or fin_inst_street_name
									or fin_inst_post_code
									or fin_inst_town_name
									or fin_inst_country_sub_div
									or fin_inst_country
									or seller_account_name
									or seller_account_iban
									or seller_account_bban
									or seller_account_upic
									or seller_account_id
									or last_match_date
									or reqrd_commercial_dataset
									or reqrd_transport_dataset
									or submitr_bic
									or data_set_id
									or freight_charges_type
									or version
									or payments
									or line_items
									or routing_summaries
									or user_defined_informations
									or contacts
									or adjustments
									or taxes
									or freight_charges
									or incoterms
									or charges
									or bo_comment
									or advising_bank/abbv_name
									or advising_bank/name
									or advising_bank/address_line_1
									or advising_bank/address_line_2
									or advising_bank/dom
									or advising_bank/reference
									or seller_bank/abbv_name
									or seller_bank/name
									or seller_bank/address_line_1
									or seller_bank/address_line_2
									or seller_bank/dom
									or seller_bank/reference
									or buyer_bank/abbv_name
									or buyer_bank/name
									or buyer_bank/address_line_1
									or buyer_bank/address_line_2
									or buyer_bank/dom
									or buyer_bank/reference)">
							A not authorised element is defined. 
						</sch:report>
					</sch:rule>
				</sch:pattern>

				<!-- PO Amend - Constraints -->			


				<!-- ************************* -->
				<!-- PO Inquire Business Rules -->
				<!-- ************************* -->

				<!-- PO Inquire mandatory fields -->
				<sch:pattern 
					name="PO Inquire - Mandatory fields " 
					xmlns:sch="http://www.ascc.net/xml/schematron">
					<sch:rule context="/po_tnx_record">
						<sch:report 
							test="tnx_type_code = '13' and
									((not(company_name) or company_name = '')
									or (not(tnx_stat_code) or tnx_stat_code = '')
									or (not(product_code) or product_code = '')
									or (not(sub_tnx_type_code) or sub_tnx_type_code = '')
									or (not(free_format_text) or free_format_text = ''))">
							Some mandatory elements are missing. 
						</sch:report>
					</sch:rule>
				</sch:pattern>
				
				<!-- PO Inquire not authorised fields -->
				<sch:pattern 
					name="PO Inquire - Not authorised fields " 
					xmlns:sch="http://www.ascc.net/xml/schematron">
					<sch:rule context="/po_tnx_record">
						<sch:report 
							test="tnx_type_code = '13' and
									(template_id
									or tnx_id
									or company_id
									or entity
									or inp_user_id 
									or inp_dttm 
									or ctl_user_id 
									or ctl_dttm 
									or release_user_id 
									or release_dttm 
									or bo_inp_user_id 
									or bo_inp_dttm 
									or bo_ctl_user_id 
									or bo_ctl_dttm 
									or bo_release_user_id 
									or bo_release_dttm
									or tnx_id
									or appl_date
									or iss_date
									or last_ship_date
									or tnx_cur_code
									or tnx_amt
									or issuer_ref_id
									or buyer_abbv_name
									or buyer_name
									or buyer_bei
									or buyer_bank_bic
									or buyer_street_name
									or buyer_post_code
									or buyer_town_name
									or buyer_country_sub_div
									or buyer_country
									or buyer_reference
									or seller_abbv_name
									or seller_name
									or seller_bei
									or seller_bank_bic
									or seller_street_name
									or seller_post_code
									or seller_town_name
									or seller_country_sub_div
									or seller_reference
									or seller_country
									or bill_to_abbv_name
									or bill_to_name
									or bill_to_bei
									or bill_to_street_name
									or bill_to_post_code
									or bill_to_town_name
									or bill_to_country_sub_div
									or bill_to_country
									or ship_to_abbv_name
									or ship_to_name
									or ship_to_bei
									or ship_to_street_name
									or ship_to_post_code
									or ship_to_town_name
									or ship_to_country_sub_div
									or ship_to_country 
									or consgn_abbv_name
									or consgn_name
									or consgn_bei
									or consgn_street_name
									or consgn_post_code
									or consgn_town_name
									or consgn_country_sub_div
									or consgn_country
									or goods_desc
									or part_ship
									or tran_ship
									or nb_mismatch
									or full_match
									or total_amt
									or total_cur_code
									or total_net_amt
									or total_net_cur_code
									or order_total_amt
									or order_total_cur_code
									or order_total_net_amt
									or order_total_net_cur_code
									or accpt_total_amt
									or accpt_total_cur_code
									or accpt_total_net_amt
									or accpt_total_net_cur_code
									or liab_total_amt
									or liab_total_cur_code
									or liab_total_net_amt
									or liab_total_net_cur_code
									or fin_inst_bic
									or fin_inst_name
									or fin_inst_street_name
									or fin_inst_post_code
									or fin_inst_town_name
									or fin_inst_country_sub_div
									or fin_inst_country
									or seller_account_name
									or seller_account_iban
									or seller_account_bban
									or seller_account_upic
									or seller_account_id
									or last_match_date
									or reqrd_commercial_dataset
									or reqrd_transport_dataset
									or submitr_bic
									or data_set_id
									or freight_charges_type
									or version
									or payments
