<?xml version="1.0" encoding="iso-8859-1"?>
<module domain="customer" scope="product">
	<name>module_documentTracking</name>
	<bindingParam>operation</bindingParam>
	<bindingParam>option</bindingParam>
	<bindingParam>tnxtype</bindingParam>
	<bindingParam>mode</bindingParam>
	<bindingParam>subproductcode</bindingParam>
	<bindingParam>productcode</bindingParam>
	<methodParam>referenceid</methodParam>
	<methodParam>tnxid</methodParam>
	<staticParam>menuaction</staticParam>

	<!-- Access to Document Tracking module -->
	<action>
		<actionCode>OpenGeneralDT</actionCode>
		<class>SecureEmailPortlet</class>
		<method>openGeneralModule</method>
		<permission>se_lncds_access</permission>
		<operation>-</operation>
		<option>-</option>
		<mode>-</mode>
		<tnxid>-</tnxid>
		<referenceid>-</referenceid>
		<tnxtype>-</tnxtype>
		<menuaction>-</menuaction>
		<psml>module_se_dashboard</psml>
	</action>
	<!-- List of Document Submissions -->
	<action>
		<actionCode>OpenDocumentSubmissionList</actionCode>
		<class>LoanListPortlet</class>
		<method>listOfDocuments</method>
		<permission>se_lncds_access</permission>
		<mode>-</mode>	
		<operation>-</operation>
		<tnxid>-</tnxid>
		<option>DT</option>
		<referenceid>-</referenceid>
		<tnxtype>01</tnxtype>
		<menuaction>OpenDocumentSubmissionList</menuaction>
		<bookmarkable>true</bookmarkable>
		<favourite>true</favourite>
	</action>
	<!-- Initiate DT from scratch -->
	<action>
		<actionCode>InitiateDocumentSubmission</actionCode>
		<class>SecureEmailPortlet</class>
		<method>initiateDTFromScratch</method>
		<permission>se_lncds_access</permission>
		<mode>-</mode>	
		<operation>NEW_DOCUMENT_SUBMISSION</operation>
		<tnxid>-</tnxid>
		<option>DT</option>
		<referenceid>-</referenceid>
		<tnxtype>01</tnxtype>
		<menuaction>OpenDocumentSubmissionList</menuaction>
		<bookmarkable>true</bookmarkable>
	</action>
	<!-- Save DT -->
	<action>
		<actionCode>SaveSEDocumentTrackingInitiation</actionCode>
		<class>SecureEmailPortlet</class>
		<method>saveSEDocumentTracking</method>
		<permission>se_lncds_save</permission>
		<operation>SAVE</operation>
		<option>DT</option>
		<mode>%</mode>
		<referenceid>%</referenceid>
		<tnxid>%</tnxid>
		<tnxtype>01</tnxtype>
		<menuaction>OpenDraftDTInitiationList</menuaction>
	</action>
	
	<!-- List of DTs Drafted -->
	<action>
		<actionCode>OpenDraftDTInitiationList</actionCode>
		<class>SecureEmailListPortlet</class>
		<method>actionCodeListdef</method>
		<permission>se_lncds_access</permission>
		<mode>DRAFT</mode>	
		<operation>-</operation>
		<tnxid>-</tnxid>
		<option>DT</option>
		<referenceid>-</referenceid>
		<tnxtype>01</tnxtype>
		<menuaction>OpenDraftDTInitiationList</menuaction>
	</action>
	<!-- Opens DT from Draft -->
	<action>
		<actionCode>OpenDraftDTInitiation</actionCode>
		<class>SecureEmailPortlet</class>
		<method>openPendingSEDocumentTracking</method>
		<permission>se_lncds_save</permission>
		<mode>DRAFT</mode>
		<referenceid>%</referenceid>
		<operation>-</operation>
		<option>-</option>
		<tnxid>%</tnxid>
		<tnxtype>01</tnxtype>
		<menuaction>OpenDraftDTInitiationList</menuaction>
	</action>
	<!-- Action that Submits DT -->
	<action>
		<actionCode>SubmitSEDocumentTrackingInitiation</actionCode>
		<class>SecureEmailPortlet</class>
		<method>submitDT</method>
		<permission>se_lncds_save</permission>
		<operation>SUBMIT</operation>
		<option>DT</option>
		<mode>%</mode>
		<referenceid>%</referenceid>
		<tnxid>%</tnxid>
		<tnxtype>01</tnxtype>		
	</action>
	<!-- List of Unsigned DTs -->
	<action>
		<actionCode>OpenUnsignedDTInitiationList</actionCode>
		<class>SecureEmailListPortlet</class>
		<method>actionCodeListdef</method>
		<permission>se_lncds_access</permission>
		<mode>UNSIGNED</mode>
		<operation>-</operation>
		<tnxid>-</tnxid>
		<option>DT</option>
		<referenceid>-</referenceid>
		<tnxtype>01</tnxtype>
		<menuaction>OpenUnsignedDTInitiationList</menuaction>
		<bookmarkable>true</bookmarkable>
	</action>
	<!-- Opens Unsigned DT -->
	<action>
		<actionCode>OpenUnsignedDTInitiation</actionCode>
		<class>SecureEmailPortlet</class>
		<method>openPendingSEDocumentTracking</method>
		<permission>se_lncds_save</permission>
		<operation>-</operation>
		<option>-</option>
		<mode>UNSIGNED</mode>
		<referenceid>%</referenceid>
		<tnxid>%</tnxid>
		<tnxtype>01</tnxtype>
		<menuaction>OpenUnsignedDTInitiationList</menuaction>
	</action>
	<!-- List of Pending DTs -->
	<action>
		<actionCode>ListDTPendingTransactions</actionCode>
		<class>SecureEmailListPortlet</class>
		<method>actionCodeListdef</method>
		<permission>se_lncds_access</permission>
		<operation>TRANSACTION_MAINTENANCE</operation>
		<option>PENDING</option>
		<mode>-</mode>
		<tnxid>-</tnxid>
		<referenceid>-</referenceid>
		<tnxtype>-</tnxtype>
		<menuaction>ListDTPendingTransactions</menuaction>
	</action>
	<!-- List of DT for Inquiry -->
	<action>
		<actionCode>InquiryDTTransactions</actionCode>
		<class>SecureEmailListPortlet</class>
		<method>actionCodeListdef</method>
		<permission>se_lncds_access</permission>
		<mode>-</mode>
		<operation>LIST_INQUIRY</operation>
		<tnxid>-</tnxid>
		<option>DT</option>
		<referenceid>-</referenceid>
		<tnxtype>-</tnxtype>
		<menuaction>InquiryDTTransactions</menuaction>
		<bookmarkable>true</bookmarkable>
	</action>
	<action>
		<actionCode>InquiryDTTransactionsHistoryView</actionCode>
		<class>SecureEmailListPortlet</class>
		<method>inquiryHistory</method>
		<permission>se_lncds_access</permission>
		<operation>LIST_INQUIRY</operation>
		<option>HISTORY</option>
		<mode>-</mode>
		<referenceid>%</referenceid>
		<tnxid>-</tnxid>
		<tnxtype>-</tnxtype>
		<productcode>SE</productcode>
		<subproductcode>DT</subproductcode>
		<menuaction>InquiryDTTransactions</menuaction>
		<psml>product_dashboard_ln</psml>
		<bookmarkable>true</bookmarkable>
	</action>

	<!-- For returning Unsigned DT transactions -->
	<action>
		<actionCode>ReturnUnsignedDTInitiation</actionCode>
		<class>SecureEmailPortlet</class>
		<method>returnSE</method>
		<permission>se_lncds_save</permission>
		<operation>RETURN_TRANSACTION</operation>
		<mode>UNSIGNED</mode>
		<tnxid>%</tnxid>
		<option>DT</option>
		<referenceid>%</referenceid>
		<tnxtype>01</tnxtype>
	</action>

	<action>
		<actionCode>documentTrackingListing</actionCode>
		<class>SecureEmailPortlet</class>
		<method>-</method>
		<permission>se_lncds_access</permission>
		<mode>-</mode>	
		<operation>DocumentTrackingListing</operation>
		<tnxid>-</tnxid>
		<option>DT</option>
		<referenceid>-</referenceid>
		<tnxtype>-</tnxtype>
		<productcode>LN</productcode>
		<menuaction>-</menuaction>
	</action>
	<action>
		<actionCode>listSubmittedTransaction</actionCode>
		<class>SecureEmailPortlet</class>
		<method>-</method>
		<permission>se_lncds_access</permission>
		<mode>-</mode>	
		<operation>ListSubmittedTransaction</operation>
		<tnxid>-</tnxid>
		<option>DT</option>
		<referenceid>-</referenceid>
		<tnxtype>-</tnxtype>
		<productcode>LN</productcode>
		<menuaction>-</menuaction>
	</action>
</module>
