	</xs:annotation>
	<xs:include schemaLocation="http://www.misys.com/mtp/interfaces/xsd/common.xsd"/>
	<xs:element name="todolist_transaction_relation">
	    <xs:complexType>
	        <xs:sequence>
				<xs:element minOccurs="0" ref="todo_list_id">
					<xs:annotation>
						<xs:documentation>Todo List Id.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="tnx_id">
					<xs:annotation>
						<xs:documentation>Transaction id.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="ref_id">
