<xsl:value-of select="$displaymode"/>
</xsl:with-param>
<xsl:with-param name="show-help">Y</xsl:with-param>
<xsl:with-param name="help-label">XSL_TSU_ActvtyRptSetUpReq_UTCOffset1_Sgn_definition</xsl:with-param>
</xsl:call-template>
</div>
</xsl:template>
<xsl:template match="UTCOffset/NbOfHrs">
<xsl:param name="path"/>
<xsl:param name="readOnly"/>
<xsl:variable name="index">
<xsl:number count="NbOfHrs"/>
</xsl:variable>
<div class="formField">
<xsl:variable name="label">XSL_TSU_ActvtyRptSetUpReq_UTCOffset1_NbOfHrs_name</xsl:variable>
<xsl:call-template name="input-field">
<xsl:with-param name="label">
<xsl:value-of select="$label"/>
</xsl:with-param>
<xsl:with-param name="name">
<xsl:value-of select="concat($path,'/NbOfHrs')"/>
</xsl:with-param>
<xsl:with-param name="value">
<xsl:value-of select="text()"/>
</xsl:with-param>
<xsl:with-param name="required">Y</xsl:with-param>
<xsl:with-param name="override-displaymode">
<xsl:value-of select="$displaymode"/>
</xsl:with-param>
<xsl:with-param name="show-help">Y</xsl:with-param>
<xsl:with-param name="help-label">XSL_TSU_ActvtyRptSetUpReq_UTCOffset1_NbOfHrs_definition</xsl:with-param>
</xsl:call-template>
</div>
</xsl:template>
</xsl:stylesheet>
