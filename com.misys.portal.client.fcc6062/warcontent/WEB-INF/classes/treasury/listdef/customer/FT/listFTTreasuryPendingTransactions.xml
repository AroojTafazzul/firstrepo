<?xml version="1.0" encoding="UTF-8"?>
<listdef ordered="y" default_order="ref_id" default_order_type="d" page="50">
	
	<!-- columns -->
	<column name="action" width="3em">
		<url action="y">
			<parameter name="REFERENCE_ID" column="ref_id"/>
			<parameter name="TRANSACTION_ID" column="tnx_id"/>
			<parameter name="TRANSACTION_STAT_CODE" column="tnx_stat_code"/>
			<parameter name="PRODUCT_CODE" column="product_code"/>
			<parameter name="OPTION_CODE" binding_code="OPTION_FULLORSUMMARY"/>
			<parameter name="TRANSACTION_TYPE_CODE" column="tnx_type_code"/>
			<parameter name="SCREEN" value="FT_TREASURY_SCREEN"/>
			<parameter name="SUB_PRODUCT_CODE" column="sub_product_code"/>
		</url>
	</column>

	<column name="ref_id" align="center" width="10em" loca_key="REFERENCEID"/>
	<column name="entity" align="left" width="15%" loca_key="ENTITY"/>
	<column name="full_type" computation="sum" align="center" width="20%" loca_key="TYPE">
		<column name="tnx_type_code"/>
		<column name="sub_tnx_type_code"/>
	</column>
	<column name="status" align="left" width="20%" loca_key="STATUS" sort="y">
		<script>
			if (value.equals(""))
			{
				var tnxStatCode = row.getColumnValue("tnx_stat_code");
				var subTnxStatCode = row.getColumnValue("sub_tnx_stat_code");
				if(("99").equals(""+subTnxStatCode+"")|| ("").equals(""+subTnxStatCode+""))
				{
					value = com.misys.portal.common.localization.Localization.getDecode(language,"N004",""+tnxStatCode+"");
				}
				else
				{
					value = com.misys.portal.common.localization.Localization.getDecode(language,"N004",""+tnxStatCode+"") +" ("+ com.misys.portal.common.localization.Localization.getDecode(language,"N015",""+subTnxStatCode+"")+")"; 
				}
			}
			value;
		</script>
	</column>
	<column name="counterparty_name" align="center" width="35%" loca_key="BENEFICIARY_NAME"/>
	<column name="cur_code" align="center" width="4em" loca_key="CURCODE"/>
	<column name="tnx_amt" align="right" width="10em" loca_key="AMOUNT"/>
	<column name="sub_product_code" align="center" width="15%" loca_key="PRODUCT_SUBTYPE"/>
	<column name="inp_dttm" align="center" width="6em" loca_key="INP_DTTM"/>

	<column name="tnx_id" hidden="y"/>
	<column name="product_code" hidden="y"/>
	<column name="tnx_type_code" hidden="y"/>
	<column name="sub_tnx_type_code" hidden="y"/>
	<column name="ObjectDataNumber@counterparty_nb" hidden="y"/>
	<column name="sub_tnx_stat_code" hidden="y"/>		
	<column name="tnx_stat_code" hidden="y"/>
	<column name="cust_ref_id" hidden="y"/>

	<!-- parameters -->
	<parameter
		name="ref_id"
		size="14"
		max_length="20"
		loca_key="REFERENCEID"
		input="y"/>
	<parameter
		name="entity"
		size="35"
		max_length="35"
		loca_key="ENTITY"
		input="y"
		default=""
		type="entity"/>
	<parameter
	    name="sub_product_code"
	    loca_key="PRODUCT_SUBTYPE"
	    type="AvaliableSubProductType"
	    input="y"
	    multiple_values="TRINT,TRTPT"/>		    
	<parameter
		name="cur_code"
		loca_key="CURCODE"
		size="3"
		max_length="3"
		input="y"
		type="currency"/>
	<parameter
		name="counterparty_name"
		loca_key="BENEFICIARY_NAME"
		size="35"
		max_length="60"
		input="y"/>	

	<parameter
		name="export_list"
		file_name="pendingFT"
		type="export"/>

	<!-- candidates -->	
	<candidate name="fund_transfer">
			<filter>
				<criteria>
					<column name="ref_id"/>
					<operator type="like"/>
					<value type="parameter">
						ref_id
					</value>
				</criteria>
				<criteria>
					<column name="entity"/>
					<operator type="like"/>
					<value type="parameter">
						entity
					</value>
				</criteria>
				<criteria>
						<column name="counterparty_name"/>
						<operator type="like"/>
						<value type="parameter">
							counterparty_name
						</value>
					</criteria>
					<criteria>
						<column name="cust_ref_id"/>
						<operator type="like"/>
						<value type="parameter">
							cust_ref_id
						</value>
					</criteria>
					<criteria>
						<column name="cur_code"/>
						<operator type="like"/>
						<value type="parameter">
							cur_code
						</value>
					</criteria>
					<criteria>
						<column name="sub_product_code"/>
						<operator type="equal"/>
						<value type="parameter">
							sub_product_code
						</value>
					</criteria>
					
					<criteria>
						<column name="sub_product_code"/>
						<operator type="in"/>
						<value type="vector">
							<value type="code">
								N047_TREASURY_TRANSFER_INTERNAL
							</value>
								<value type="code">
								N047_TREASURY_PAYMENT_EXTERNAL
							</value>			
						</value>
					</criteria>
			</filter>
	</candidate>
	
	<filter or="y">
		<criteria>
			<column name="tnx_type_code"/>
			<operator type="equal"/>
			<value type="code">
				N002_NEW
			</value>
		</criteria>
		<criteria>
			<column name="tnx_type_code"/>
			<operator type="equal"/>
			<value type="code">
				N002_AMEND
			</value>
		</criteria>
		<criteria>
			<column name="tnx_type_code"/>
			<operator type="equal"/>
			<value type="code">
				N002_INQUIRE
			</value>
		</criteria>
	</filter>
	
  <filter or="y">
    <criteria>
      <column name="tnx_stat_code"/>
      <operator type="different"/>
      <value type="code">
        N004_ACKNOWLEDGED
      </value>
    </criteria>
    <criteria>
      <column name="prod_stat_code"/>
      <operator type="equal"/>
      <value type="code">
        N005_INPROGRESS
      </value>
    </criteria>
  </filter>

</listdef>