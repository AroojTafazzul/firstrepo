						<xs:documentation>Charging account address line 1.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="charge_account_address_line_2">
					<xs:annotation>
						<xs:documentation>Charging account address line 2.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="charge_account_address_line_3">
					<xs:annotation>
						<xs:documentation>Charging account address line 3.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="charge_account_address_line_4">
					<xs:annotation>
						<xs:documentation>Charging account address line 4.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="check_duplicate_cust_ref">
					<xs:annotation>
						<xs:documentation>Check duplicate customer reference flag(Y/N).</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="check_duplicate_file">
					<xs:annotation>
						<xs:documentation>Check duplicate upload file flag(Y/N).</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="check_file_hash_value">
					<xs:annotation>
						<xs:documentation>Check file hash value flag(Y/N).</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="company_group">
					<xs:annotation>
						<xs:documentation>Group the company belongs to.</xs:documentation>
