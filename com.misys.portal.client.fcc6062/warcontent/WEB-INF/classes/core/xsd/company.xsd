<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	elementFormDefault="qualified">
	<xs:annotation>
		<xs:documentation xml:lang="en"> Schema W3C for a GTP company. XML validation 
			Copyright (c) 2000-2011 Misys (http://www.misys.com), All 
			Rights Reserved. </xs:documentation>
	</xs:annotation>
	<xs:include schemaLocation="http://www.misys.com/portal/interfaces/xsd/common.xsd"/>
	<xs:element name="company">
	    <xs:complexType>
	        <xs:sequence>
				<xs:element minOccurs="0" ref="maker_id">
					<xs:annotation>
						<xs:documentation>Maker Id.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="checker_id">
					<xs:annotation>
						<xs:documentation>Checker Id.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="tnx_id">
					<xs:annotation>
						<xs:documentation>Transaction Id.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="maker_dttm">
					<xs:annotation>
						<xs:documentation>Maker datetime.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="checker_dttm">
					<xs:annotation>
						<xs:documentation>Checker datetime.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="tnx_type_code">
					<xs:annotation>
						<xs:documentation>Transaction type code.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="tnx_stat_code">
					<xs:annotation>
						<xs:documentation>Transaction status code.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="return_comments">
					<xs:annotation>
						<xs:documentation>Return comments.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="company_id">
					<xs:annotation>
						<xs:documentation>Company Id.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="1" ref="abbv_name">
					<xs:annotation>
						<xs:documentation>Abbreviated name.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="1" ref="actv_flag">
					<xs:annotation>
						<xs:documentation>Active flag.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="address_line_1">
					<xs:annotation>
						<xs:documentation>First line of address.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="address_line_2">
					<xs:annotation>
						<xs:documentation>Second line of address.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="attachment_max_upload_size">
					<xs:annotation>
						<xs:documentation>Max size for attachment upload(in MB).</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="authorize_own_transaction">
					<xs:annotation>
						<xs:documentation>Authorize own transaction flag (Y/N).</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="auto_fwd_date">
					<xs:annotation>
						<xs:documentation>Auto forward date flag (Y/N).</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="1" ref="base_cur_code">
