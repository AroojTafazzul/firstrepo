						<xs:documentation>Legal id number.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="legal_id_type">
					<xs:annotation>
						<xs:documentation>Legal id type.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="merge_demerge_allowed">
					<xs:annotation>
						<xs:documentation>Merge demerge allowed for bulk flag(Y/N).</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="1" ref="name">
					<xs:annotation>
						<xs:documentation>Name.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="owner_id">
					<xs:annotation>
						<xs:documentation>Owner company Id.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="password_expiry">
					<xs:annotation>
						<xs:documentation>Number of days after which the password expires.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="phone">
					<xs:annotation>
						<xs:documentation>Phone number.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="post_code">
					<xs:annotation>
						<xs:documentation>Post code.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="reference">
					<xs:annotation>
						<xs:documentation>Reference.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="reject_file_on_error">
					<xs:annotation>
						<xs:documentation>Reject bulk file on error.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="retention_period">
