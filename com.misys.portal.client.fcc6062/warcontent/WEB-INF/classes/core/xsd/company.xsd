						<xs:documentation>Treasury branch reference.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="liquidity_frequency">
					<xs:annotation>
						<xs:documentation>Liquidity Frequency.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="liquidity_balance_type">
					<xs:annotation>
						<xs:documentation>Liquidity Balance Type.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="liquidity_ccy_cur_code">
					<xs:annotation>
						<xs:documentation>Liquidity Currency.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="created">
					<xs:annotation>
						<xs:documentation>Company creation date and time.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="psml_template">
					<xs:annotation>
						<xs:documentation>Name of the PSML template used for the homepage of users of the company.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="internal_channel">
					<xs:annotation>
						<xs:documentation>Channel ID of the Internal News Channel.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="1" ref="owner_abbv_name">
					<xs:annotation>
						<xs:documentation>Owner abbreviated name.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" maxOccurs="unbounded" ref="additional_field"/>
