						<xs:documentation>Town name.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="1" ref="type">
					<xs:annotation>
						<xs:documentation>Type of the company.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="web_address">
