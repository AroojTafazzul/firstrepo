						<xs:documentation>Locked time.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element minOccurs="0" ref="company_default_user">
			  		<xs:annotation>
						<xs:documentation>Company default user flag(Y/N).</xs:documentation>
					</xs:annotation>
				</xs:element>
