<?xml version="1.0" encoding="UTF-8"?>
<listdef ordered="y" huge="y" page="50" default_order="ref_id" default_order_type="d" js_binding="misys.binding.common.listdef_mc">

	<column name="ref_id" align="center" width="10em" loca_key="REFERENCEID">
		<url candidate="bulk_master">
			<parameter name="SCREEN" value="BK_SCREEN"/>
			<parameter name="TRANSACTION_TYPE_CODE" business_code="N002_NEW"/>
			<parameter name="OPTION_CODE" binding_code="OPTION_EXISTING"/>
			<parameter name="REFERENCE_ID" column="ref_id"/>
		</url>
		<url candidate="bulk">
			<parameter name="SCREEN" value="BK_SCREEN"/>
			<parameter name="TRANSACTION_TYPE_CODE" business_code="N002_NEW"/>
			<parameter name="OPTION_CODE" binding_code="OPTION_REJECTED"/>
			<parameter name="REFERENCE_ID" column="ref_id"/>
			<parameter name="TRANSACTION_ID" column="tnx_id"/>				
		</url>
	</column>
	<column name="entity" align="left" width="20%" loca_key="ENTITY"/>
	<column name="applicant_name" align="left" width="20%" loca_key="ENTITY_NAME_APPLICANT"/>
	<column name="bo_ref_id" align="left" width="20%" loca_key="BO_REFERENCEID"/>
	<column name="cust_ref_id" align="left" width="20%" loca_key="BULK_CUSTOMER_REFERENCE"/>
	<column name="record_number" align="center" width="5em" loca_key="RECORDS"/>
	<column name="cur_code" align="center" width="4em" loca_key="CURCODE"/>
	<column name="amt" align="right" width="10em" loca_key="AMOUNT"/>
	<column name="product_code" hidden="y"/>
	<column name="tnx_id" hidden="y"/>
	<column name="sub_product_code" hidden="y"/>	
	<!-- parameters -->
	<parameter
		name="ref_id"
		size="20"
		max_length="20"
		loca_key="REFERENCEID"
		input="y"
		default="">
	</parameter>
	<parameter
		name="entity"
		size="20"
		max_length="35"
		loca_key="ENTITY"
		input="y"
		default=""
		type="entity"/>
	<parameter
		name="applicant_name"
		size="35"
		max_length="35"
		loca_key="ENTITY_NAME_APPLICANT"
		input="y"
		default=""
		/>
	<parameter
		name="bo_ref_id"
		loca_key="BO_REFERENCEID"
		size="20"
		max_length="35"
		input="y"/>
	<parameter
		name="cur_code"
		input="y"
		size="3"
		max_length="3"
		loca_key="CURCODE"
		type="currency"/>

	<!-- candidates -->	
	<candidate name="bulk_master">
		<filter>
			<criteria>
				<column name="ref_id"/>
				<operator type="like"/>
				<value type="parameter">
					ref_id
				</value>
			</criteria>
			<criteria>
				<column name="bo_ref_id"/>
				<operator type="like"/>
				<value type="parameter">
					bo_ref_id
				</value>
			</criteria>
			<criteria>
				<column name="entity"/>
				<operator type="like"/>
				<value type="parameter">
					entity
				</value>
			</criteria>
			<criteria>
				<column name="applicant_name"/>
				<operator type="like"/>
				<value type="parameter">
					applicant_name
				</value>
			</criteria>
			<criteria>
				<column name="beneficiary_name"/>
				<operator type="like"/>
				<value type="parameter">
					beneficiary_name
				</value>
			</criteria>
			<criteria>
				<column name="cur_code"/>
				<operator type="like"/>
				<value type="parameter">
					cur_code
				</value>
			</criteria>
			<criteria>
				<column name="prod_stat_code"/>
				<operator type="different"/>
				<value type="code">
					N005_PURGED
				</value>
			</criteria>
		</filter>
	</candidate>

	<candidate name="bulk">
		<filter>
			<criteria>
				<column name="ref_id"/>
				<operator type="like"/>
				<value type="parameter">
					ref_id
				</value>
			</criteria>
			<criteria>
				<column name="bo_ref_id"/>
				<operator type="like"/>
				<value type="parameter">
					bo_ref_id
				</value>
			</criteria>
			<criteria>
				<column name="applicant_name"/>
				<operator type="like"/>
				<value type="parameter">
					applicant_name
				</value>
			</criteria>
			<criteria>
				<column name="beneficiary_name"/>
				<operator type="like"/>
				<value type="parameter">
					beneficiary_name
				</value>
			</criteria>
			<criteria>
				<column name="cur_code"/>
				<operator type="like"/>
				<value type="parameter">
					cur_code
				</value>
			</criteria>
			<criteria>
				<column name="prod_stat_code"/>
				<operator type="equal"/>
				<value type="code">
					N005_REJECTED
				</value>
			</criteria>
			<criteria>
				<column name="tnx_type_code"/>
				<operator type="equal"/>
				<value type="code">
					N002_NEW
				</value>
			</criteria>
			<criteria>
				<column name="tnx_stat_code"/>
				<operator type="equal"/>
				<value type="code">
					N004_ACKNOWLEDGED
				</value>
			</criteria>
		</filter>
	</candidate>
	<filter and="y">
			<criteria>
				<column name="sub_product_code"/>
				<operator type="not in"/>
				<value type="vector">
					<value type="code">
						N047_BK_LOAN_REPRICING
					</value>
					<value type="code">
						N047_BK_FEE_PAYMENT
					</value>
				</value>
			</criteria>
	</filter>		
	
</listdef>
