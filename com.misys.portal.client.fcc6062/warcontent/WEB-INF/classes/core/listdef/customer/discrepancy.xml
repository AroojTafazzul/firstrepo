<?xml version="1.0" encoding="UTF-8"?>
<listdef ordered="y" default_order="ref_id" default_order_type="d" page="10">

	<column name="ref_id" align="center" width="10em" loca_key="REFERENCEID">
		<url>
			<parameter name="SCREEN" column="product_code"/>
			<parameter name="TRANSACTION_TYPE_CODE" business_code="N002_INQUIRE"/>
			<parameter name="OPTION_CODE" binding_code="OPTION_EXISTING"/>
			<parameter name="MODE_CODE" binding_code="MODE_DISCREPANT"/>
			<parameter name="REFERENCE_ID" column="ref_id"/>
			<parameter name="TRANSACTION_ID" column="tnx_id"/>
		</url>
	</column>
	<column name="entity" align="left" width="30%" loca_key="ENTITY"/>
	<!-- column name="bo_ref_id" align="left" width="30%" loca_key="BO_REFERENCEID"/-->
	<column name="beneficiary_name" align="left" width="40%" loca_key="BENEFICIARY_NAME"/>
	<column name="cur_code" align="center" width="4em" loca_key="CURCODE"/>
	<column name="tnx_amt" align="right" width="10em" loca_key="AMOUNT"/>
	<!-- column name="iss_date" align="center" width="6em" loca_key="ISS_DATE"/>
	<column name="exp_date" align="center" width="6em" loca_key="EXP_DATE"/-->
	
	
	<column name="tnx_id" hidden="y"/>
	<column name="product_code" hidden="y"/>
	
	<!-- parameters -->
	<parameter
		name="ref_id"
		size="14"
		max_length="20"
		loca_key="REFERENCEID"
		input="y"
		default="">
	</parameter>
	<parameter
		name="entity"
		size="35"
		max_length="35"
		loca_key="ENTITY"
		input="y"
		default=""/>
	<parameter
		name="bo_ref_id"
		loca_key="BO_REFERENCEID"
		size="20"
		max_length="35"
		input="y"/>
	<parameter
		name="beneficiary_name"
		loca_key="BENEFICIARY_NAME"
		size="35"
		max_length="35"
		input="y"/>
	<parameter
		name="cur_code"
		input="y"
		size="3"
		max_length="3"
		loca_key="CURCODE"/>

	<!-- candidates -->	
	<candidate name="letter_of_credit">
		<filter>
			<criteria>
				<column name="ref_id"/>
				<operator type="like"/>
				<value type="parameter">
					ref_id
				</value>
			</criteria>
			<criteria>
				<column name="entity"/>
				<operator type="like"/>
				<value type="parameter">
					entity
				</value>
			</criteria>
			<criteria>
				<column name="bo_ref_id"/>
				<operator type="like"/>
				<value type="parameter">
					bo_ref_id
				</value>
			</criteria>
			<criteria>
				<column name="beneficiary_name"/>
				<operator type="like"/>
				<value type="parameter">
					beneficiary_name
				</value>
			</criteria>
			<criteria>
				<column name="cur_code"/>
				<operator type="like"/>
				<value type="parameter">
					cur_code
				</value>
			</criteria>
		</filter>
	</candidate>
	
	<candidate name="standby_issued">
		<filter>
			<criteria>
				<column name="ref_id"/>
				<operator type="like"/>
				<value type="parameter">
					ref_id
				</value>
			</criteria>
			<criteria>
				<column name="entity"/>
				<operator type="like"/>
				<value type="parameter">
					entity
				</value>
			</criteria>
			<criteria>
				<column name="bo_ref_id"/>
				<operator type="like"/>
				<value type="parameter">
					bo_ref_id
				</value>
			</criteria>
			<criteria>
				<column name="beneficiary_name"/>
				<operator type="like"/>
				<value type="parameter">
					beneficiary_name
				</value>
			</criteria>
			<criteria>
				<column name="cur_code"/>
				<operator type="like"/>
				<value type="parameter">
					cur_code
				</value>
			</criteria>
		</filter>
	</candidate>
	
	<filter>
		<criteria>
			<column name="action_req_code"/>
			<operator type="equal"/>
			<value type="code">
				N042_DISCREPANCY_RESPONSE
			</value>
		</criteria>
		<criteria>
			<column name="CrossReference@child_tnx_id"/>
			<operator type="isNull"/>
		</criteria>
		<criteria>
			<column name="CrossReference@type_code"/>
			<operator type="equal"/>
			<value type="code">
				N043_ACTION_REQUIRED
			</value>
		</criteria>
	</filter>
	
</listdef>
