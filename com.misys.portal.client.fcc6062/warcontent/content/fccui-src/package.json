{
  "name": "fccui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:local": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng s --poll 1000",
    "startES5": "ng serve --configuration=es5",
    "buildES5": "ng build --configuration=es5",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint-check": "ng lint --eslint-config=.eslintrc-dev.json",
    "lint-fix": "ng lint --eslint-config=.eslintrc-dev.json --fix",
    "e2e": "ng e2e",
    "dev-es": "ng lint --eslint-config=.eslintrc-dev.json",
    "delete-lib": "rimraf dist/fccui-lib/",
    "packagr": "npm run delete-lib && ng-packagr -p ng-package.json -c tsconfig.json && npm run build:scss",
    "postinstall": "ngcc",
    "build:scss": "scss-bundle -p scss-bundle.config.json",
    "prepare:husky": "cd .. && npx husky install com.finastra.portal.ui/.husky",
    "pre-commit": "lint-staged",
    "webpack:build": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=production --stats-json",
    "webpack:stats": "webpack-bundle-analyzer dist/fccui/stats.json",
    "sourcemap:build": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=production --source-map",
    "sourcemap:report": "source-map-explorer dist/fccui/**/*.js"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --config .eslintrc-dev.json --fix"
    ]
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
    "@angular/compiler-cli": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/forms": "^12.2.16",
    "@angular/material": "11.2.13",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/router": "^12.2.16",
    "@ffdc/uxg-angular-components": "^0.23.8",
    "@ffdc/uxg-angular-theme": "^0.23.8",
    "@fullcalendar/core": "^5.5.1",
    "@kolkov/angular-editor": "^0.13.1",
    "@ng-idle/core": "^10.0.0-beta.1",
    "@ng-idle/keepalive": "^10.0.0-beta.1",
    "@nguniversal/express-engine": "^11.0.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@types/chart.js": "^2.9.29",
    "@types/jasmine": "~3.6.0",
    "@types/dompurify": "^2.3.3",
    "@types/moment": "^2.13.0",
    "@types/trusted-types": "^2.0.2",
    "bn-ng-idle": "0.0.2",
    "chart.js": "^2.9.4",
    "chartjs-plugin-datalabels": "^0.7.0",
    "classlist.js": "^1.1.20150312",
    "css-vars-ponyfill": "^2.4.3",
    "dompurify": "^2.3.6",
    "exceljs": "^4.2.1",
    "file-saver": "^2.0.5",
    "font-awesome": "^4.7.0",
    "isomorphic-fetch": "^2.2.1",
    "jspdf": "^2.3.0",
    "jspdf-autotable": "^3.5.13",
    "material-design-icons": "^3.0.1",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "ng-idle": "^1.3.2",
    "ng-packagr": "^12.2.7",
    "ng-recaptcha": "^7.0.1",
    "ng2-charts": "^2.4.3",
    "ngx-cookie": "^5.0.0",
    "powerbi-client": "^2.16.5",
    "primeflex": "^2.0.0",
    "primeng": "^9.1.3",
    "quill": "^1.3.7",
    "recordrtc": "^5.6.1",
    "roboto-fontface": "^0.10.0",
    "rxjs": "~6.5.5",
    "trusted-types": "^2.0.0",
    "tslib": "^2.0.0",
    "tslint": "^6.1.3",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.4",
    "ngx-extended-pdf-viewer": "^10.5.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/build-angular": "~12.2.17",
    "@angular-eslint/builder": "1.2.0",
    "@angular-eslint/eslint-plugin": "1.2.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/schematics": "1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/cli": "^12.2.17",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-rxjs": "^5.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^12.1.3",
    "scss-bundle": "^3.1.2",
    "typescript": "~4.3.5"
  }
}
